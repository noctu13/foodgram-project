server{
    listen ${NGINX_PORT};
    server_name ${NGINX_IP};
    location /static/ {
        autoindex on;
        alias /usr/share/nginx/html/static/;
    }
    location /media/ {
        autoindex on;
        alias /usr/share/nginx/html/media/;
    }
    location / {
        proxy_pass ${NGINX_HOST};
    }
}
